import java.util.*;
class Main {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        String str=sc.nextLine();
        char ch[]=str.toCharArray();
        char max=Character.MIN_VALUE;
        for(int i=0;i<ch.length;i++){
            if(max<ch[i]){
               max=ch[i];
            }
        }
        System.out.println(max);
    }
}
