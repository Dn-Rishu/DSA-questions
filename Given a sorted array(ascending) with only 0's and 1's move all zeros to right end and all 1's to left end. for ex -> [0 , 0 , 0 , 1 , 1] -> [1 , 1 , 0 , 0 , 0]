import java.util.*;
class Main {
    public static void main(String[] args) {
    Scanner sc=new Scanner(System.in);
    System.out.println("enter the array:");
    int n=sc.nextInt();
    int arr[]=new int[n];
    System.out.println("enter the element:");
    for(int i=0;i<n;i++){
        arr[i]=sc.nextInt();
    }
    int count=0;
  for(int i=0;i<n;i++){
     if(arr[i]==1){
         count++;
     } 
  }
  for(int i=0;i<n;i++){
      if(i<count){
          arr[i]=1;
      }
      else{
          arr[i]=0;
      }
  }
  for(int i=0;i<n;i++){
      System.out.println(arr[i]);
  }
    }
}
