import java.util.*;
class Main {
    public static void main(String[] args) {
       Scanner sc=new Scanner(System.in);
       System.out.println("enter the size of element");
       int n=sc.nextInt();
       int arr[]=new int[n];
       System.out.println("enter the element");
       for(int i=0;i<n;i++){
           arr[i]=sc.nextInt();
       }
       List<Integer>ls=new ArrayList<>();
       for(int i=0;i<n;i++){
           if(arr[i]==2){
           continue;
           }
             ls.add(arr[i]);
       }
       System.out.println(ls);
    }
}
