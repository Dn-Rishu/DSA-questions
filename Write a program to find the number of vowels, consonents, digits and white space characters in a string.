import java.util.*;
class Main {
    public static void main(String[] args) {
    Scanner sc=new Scanner(System.in);
    String str=sc.nextLine();
    int vow=0;
    int cont=0;
    int white=0;
    int digit=0;
    for(int i=0;i<str.length();i++){
        if(str.charAt(i)=='a'||str.charAt(i)=='e'||str.charAt(i)=='i'||str.charAt(i)=='o'||str.charAt(i)=='u'){
            vow++;
        }
        if(str.charAt(i)==' '){
            white++;
        }
        if(str.charAt(i)>=48 && str.charAt(i)<=57){
            digit++;
        }
        else{
           cont++; 
        }
    }
    System.out.println("the number of vowels "+vow+"the number of consonent "+cont+"the number of digits "+digit+"the number of white spaces "+white);
    }
}
