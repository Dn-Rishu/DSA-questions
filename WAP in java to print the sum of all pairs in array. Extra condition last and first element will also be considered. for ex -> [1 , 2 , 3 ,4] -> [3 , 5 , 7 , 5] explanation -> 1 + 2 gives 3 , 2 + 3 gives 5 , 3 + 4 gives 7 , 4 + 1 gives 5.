import java.util.*;
class Main {
    public static void main(String[] args) {
      Scanner sc=new Scanner(System.in);
      System.out.println("enter the size of array:");
      int n=sc.nextInt();
      int arr[]=new int[n];
      System.out.println("enter the element:");
      for(int i=0;i<n;i++){
          arr[i]=sc.nextInt();
      }
      int arr1[]=new int[n];
     for(int i=0;i<n;i++){
         if(i==n-1){
             arr1[i]=arr[i]+arr[0];
         }
         else{
             arr1[i]=arr[i]+arr[i+1];
         }
     } 
     for(int i=0;i<n;i++){
    System.out.println(arr1[i]); 
    }
    }
}
