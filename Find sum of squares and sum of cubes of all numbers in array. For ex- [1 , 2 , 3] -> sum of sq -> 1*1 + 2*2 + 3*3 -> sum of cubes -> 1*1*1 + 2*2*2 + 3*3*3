import java.util.*;
class Main {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int arr[]=new int[n];
        for(int i=0;i<n;i++){
            arr[i]=sc.nextInt();
        }
       int sum1=0;
       int sum2=0;
       for(int i=0;i<n;i++){
           sum1+=arr[i]*arr[i];
           sum2+=arr[i]*arr[i]*arr[i];
       }
       System.out.println("the sum of squares is: "+sum1);
       System.out.println("the sum of cubes is: "+sum2);
    }
}
