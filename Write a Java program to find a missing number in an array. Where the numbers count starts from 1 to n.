import java.util.*;
class Main {
    public static void main(String[] args) {
    Scanner sc=new Scanner(System.in);
    System.out.println("enter the array:");
    int n=sc.nextInt();
    int arr[]=new int[n];
    System.out.println("enter the element:");
    for(int i=0;i<n;i++){
        arr[i]=sc.nextInt();
    }
   int j=1;
 System.out.println("the missing element is:");
for(int i=0;i<n;i++){
     if(j!=arr[i]){
         System.out.println(j);
         break;
     }
     j++;
 }
    }
}
