import java.util.*;
class Main {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int arr[]=new int[n];
        for(int i=0;i<n;i++){
            arr[i]=sc.nextInt();
        }
       int sum=0;
       for(int i=0;i<n;i++){
           sum+=arr[i];
       }
       System.out.println("the sum of elements is: "+sum);
       float avg=sum/n;
       System.out.println("the avg of elements is: "+avg);
    }
}
