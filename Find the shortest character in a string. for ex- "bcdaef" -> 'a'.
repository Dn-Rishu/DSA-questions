import java.util.*;
class Main {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        String str=sc.nextLine();
        char ch[]=str.toCharArray();
        char min=Character.MAX_VALUE;
        for(int i=0;i<ch.length;i++){
            if(min>ch[i]){
               min=ch[i];
            }
        }
        System.out.println(min);
    }
}
