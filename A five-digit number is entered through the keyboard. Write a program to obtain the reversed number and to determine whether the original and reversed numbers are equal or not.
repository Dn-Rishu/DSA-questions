import java.util.*;
class Main {
    public static void main(String[] args) {
     Scanner sc=new Scanner(System.in);
     int num=sc.nextInt();
     int orig=num;
     int rev=0;
     while(num!=0){
         int rem=num%10;
         rev=(rev*10)+rem;
         num=num/10;
     }
     if(orig==rev){
     System.out.println("both are equal");
    }
    else{
        System.out.println("not equal");
    }
}
}




